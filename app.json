{
  "expo": {
    "name": "Ghumne",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Flag_of_Nepal.svg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#87cefa"   // Light blue background color
      // You can also add an image here if you want:
      // "image": "./assets/splash.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1ea82f60-9200-4eb9-ae42-db99eb73053b"
      }
    },
    "android": {
      "package": "com.amritsapkota.exponative"
    }
  }
}
